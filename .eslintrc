{
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": 0,
    "max-len": [1, 120, 2, {
      "ignoreComments": true}],
    "react/prop-types": 2
  },
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["airbnb-base", "plugin:react/recommended"]
}
